@using WebApplication1.Models;
@{
    List<LoginContact> loginContacts = ViewBag.loginContacts;
}}

@*{
      foreach (LoginContact loginContact in loginContacts) {

      <div>
          <span>@loginContact.LoginNum</span>
          <span>@loginContact.INDO</span>
          <span>@loginContact.PW</span>
      </div>

       }

    }*@

<table class="table">
    <thead>
        <tr>
            <th>編號</th>
            <th>身分證字號</th>
            <th>密碼</th>
            <th></th>
        </tr>
    </thead>
    <tbody>


        @foreach (LoginContact loginContact in loginContacts)
        {
            <tr>
                <th>@loginContact.LoginNum</th>
                <th>@loginContact.INDO</th>
                <th>@loginContact.PW</th>
                <th>
                <th>
            </tr>
        }

    </tbody>
</table>
