
@{
    ViewBag.Title = "CreateLoginContact";
}

<h2>CreateLoginContact</h2>
<br />

<form method="post" action="/Home/CreateLoginContact">
    <div class="form-group row">
        <label for="inputINDO" class="col-sm-2">身分證字號</label>
        <div class="col-sm-10">
            <input type="text" id="inputINDO" name="INDO" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label for="inputPW" class="col-sm-2">密碼</label>
        <div class="col-sm-10">
            <input type="text" id="inputPW" name="PW" class="form-control" />
        </div>
    </div>
    <input class="btn btn-default" type="submit" value="新增" />
</form>

